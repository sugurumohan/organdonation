doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    title User List
    link(rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css")

  body
    .container.mt-5
      h1 User List with the organ 
      table.table.table-bordered
        thead
          tr
            th Name
            th age
            th phone
        tbody 
         each user in data1
              tr
                td= user.name
                td= user.age
                td=user.phonenumber
